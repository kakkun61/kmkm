(module
  parametricPolymorphism
  (list
    kmkm.prim)
  (list
    (bind-value id (for-all a (function a a a)))
    (bind-value hello (apply (instantiate id kmkm.prim.string) "Hello world!"))
    (bind-value zero (apply (instantiate id kmkm.prim.int) 0))
    (define solo (for-all a (list (solo (list (item a))))))))
